<% @libraries.each do |library| %>
  <div>
    <h3><%= library.name %></h3>
    <% if @current_user %>
      <%= button_to "Join", library_users_path(library) %>
      <%= button_to "Edit", edit_library_path(library), :method => :get %>
    <% end %>
  </div>
  <br>
<% end %>

<%= button_to 'Add Library', new_library_path, :method => :get %><br>

<%= button_to 'Logout', logout_path, method: :delete %>
